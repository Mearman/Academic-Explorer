{
  "name": "@academic-explorer/generators",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tools/generators",
  "projectType": "library",
  "targets": {
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
