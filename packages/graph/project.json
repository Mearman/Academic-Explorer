{
  "name": "graph",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/graph/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -b",
        "cwd": "{projectRoot}"
      },
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit",
        "cwd": "{projectRoot}"
      },
      "cache": true
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint src",
        "cwd": "{projectRoot}"
      },
      "cache": true
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "eslint src --fix",
        "cwd": "{projectRoot}"
      },
      "cache": false
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest run",
        "cwd": "{projectRoot}"
      },
      "outputs": [
        "{projectRoot}/coverage"
      ],
      "cache": true
    },
    "test:unit": {
      "executor": "nx:noop",
      "options": {},
      "cache": false
    },
    "test:integration": {
      "executor": "nx:noop",
      "options": {},
      "cache": false
    },
    "test:component": {
      "executor": "nx:noop",
      "options": {},
      "cache": false
    },
    "test:watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vitest",
        "cwd": "{projectRoot}"
      },
      "cache": false
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf dist",
        "cwd": "{projectRoot}"
      },
      "cache": false
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -b --watch",
        "cwd": "{projectRoot}"
      },
      "cache": false
    }
  },
  "tags": ["type:library", "scope:graph", "layer:core"]
}