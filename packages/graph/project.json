{
  "name": "graph",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/graph/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "pnpm build",
        "cwd": "packages/graph"
      },
      "dependsOn": ["types:build", "utils:build"]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "tsc --noEmit -p tsconfig.typecheck.json",
        "cwd": "packages/graph"
      },
      "dependsOn": ["types:build", "utils:build"]
    }
  },
  "tags": ["type:library", "scope:graph", "layer:core"]
}
