{
  "name": "types",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/types/entities/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "vite build",
        "cwd": "{projectRoot}"
      },
      "dependsOn": ["barrelsby"]
    },
    "barrelsby": {
      "executor": "nx:run-commands",
      "options": {
        "command": "barrelsby --config ../../.barrelsby.json --directory ./src",
        "cwd": "{projectRoot}"
      },
      "cache": true
    },
    "test:unit": {},
    "test:component": {},
    "test:integration": {}
  },
  "tags": [
    "type:library",
    "scope:shared",
    "type:entity"
  ]
}
