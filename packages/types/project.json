{
  "name": "types",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/types/src",
  "projectType": "library",
  "tags": ["type:library", "scope:shared", "type:entity"],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "configFile": "packages/types/vite.config.ts",
        "outputPath": "packages/types/dist"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "rm -rf dist",
        "cwd": "{projectRoot}"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc -b --watch",
        "cwd": "{projectRoot}"
      },
      "cache": false
    },
    "validate": {}
  }
}
