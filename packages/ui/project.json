{
  "name": "ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/ui/src",
  "projectType": "library",
  "targets": {
    "barrelsby": {},
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{projectRoot}/dist"
      ],
      "options": {
        "configFile": "packages/ui/vite.config.ts",
        "outputPath": "packages/ui/dist"
      },
      "dependsOn": [
        "types:build",
        "utils:build"
      ]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "tsc --noEmit -p tsconfig.typecheck.json",
        "cwd": "packages/ui"
      },
      "dependsOn": ["barrelsby", "^build"],
      "cache": true
    }
  },
  "tags": [
    "type:library",
    "scope:ui",
    "layer:ui"
  ]
}
