{
  "name": "utils",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/utils/src",
  "projectType": "library",
  "targets": {
    "barrelsby": {},
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": [
        "{projectRoot}/dist"
      ],
      "options": {
        "main": "packages/utils/src/index.ts",
        "outputPath": "packages/utils/dist",
        "tsConfig": "packages/utils/tsconfig.json",
        "assets": []
      },
      "dependsOn": [
        "types:build"
      ]
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "tsc --noEmit -p tsconfig.json",
        "cwd": "packages/utils"
      },
      "dependsOn": ["barrelsby", "^build"],
      "cache": true
    }
  },
  "tags": [
    "type:library",
    "scope:shared",
    "type:util"
  ]
}
