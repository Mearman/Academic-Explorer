{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "skipLibCheck": true,
    "allowJs": false,
    "strict": true,
    "noEmit": true,
    "types": ["node"]
  },
  "include": [
    "./**/*"
  ]
}